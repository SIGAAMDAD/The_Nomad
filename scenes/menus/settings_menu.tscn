[sub_resource type="GDScript" id="GDScript_10nm4"]
resource_name = "controls_settings"
script/source = "class_name ControlsSettings extends TabBar

@onready var _movement_binds_container: VBoxContainer = $VBoxContainer/MarginContainer/HBoxContainer/MovementContainer

func _hide_all() -> void:
	_movement_binds_container.hide()

func _on_movement_button_pressed() -> void:
	_hide_all()
	
	_movement_binds_container.show()

func _on_utility_button_pressed() -> void:
	pass

func _on_combat_button_pressed() -> void:
	pass

func _ready() -> void:
	var _movement_button: Button = get_node( \"VBoxContainer/MarginContainer/HBoxContainer/NavigationContainer/MovementButton\" )
	_movement_button.connect( \"pressed\", _on_movement_button_pressed )
	
	var _utility_button: Button = get_node( \"VBoxContainer/MarginContainer/HBoxContainer/NavigationContainer/UtilityButton\" )
	_utility_button.connect( \"pressed\", _on_utility_button_pressed )
	
	var _combat_button: Button = get_node( \"VBoxContainer/MarginContainer/HBoxContainer/NavigationContainer/CombatButton\" )
	_combat_button.connect( \"pressed\", _on_combat_button_pressed )
"

