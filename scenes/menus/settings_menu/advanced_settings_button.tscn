resource_name = "advanced_settinsg"
script/source = "class_name AdvancedSettingsButton extends Button

@export var _toggle_container: VBoxContainer

func _on_pressed() -> void:
	get_parent().hide()
	_toggle_container.show()

func _ready() -> void:
	get_parent().connect( \"visibility_changed\", func(): _toggle_container.visible = !get_parent().visible )
"

