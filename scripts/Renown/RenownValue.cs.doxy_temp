namespace Renown {
	public struct RenownValue {
		public Object Object = null;
		public float Value = 0.0f;

		public RenownValue() {
		}
		public RenownValue( in Object value ) {
			Object = value;
		}
		public RenownValue( in Object value, in float nScore ) {
			Object = value;
			Value = nScore;
		}
	};
};