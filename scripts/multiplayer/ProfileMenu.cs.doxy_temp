using Godot;
using Multiplayer;

public partial class ProfileMenu : Control {
	public override void _Ready() {
		base._Ready();

		Profile.Load();

		Label Name = GetNode<Label>( "VBoxContainer/NameLabel" );
		Name.Text = Profile.Instance.Name;

		Label SkillPoints = GetNode<Label>( "VBoxContainer/SkillPoints" );
		SkillPoints.Text = Profile.Instance.SkillPoints.ToString();
	}
};
